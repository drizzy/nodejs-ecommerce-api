    "@types/node": "^18.16.2",
    "@types/nodemailer": "^6.4.8",
    "@types/paypal__checkout-server-sdk": "^1.0.5",
    "@types/pg": "^8.6.6",
    "@types/stripe": "^8.0.417",
    "@types/uuid": "^9.0.4",
    "mocha": "^10.2.0"
